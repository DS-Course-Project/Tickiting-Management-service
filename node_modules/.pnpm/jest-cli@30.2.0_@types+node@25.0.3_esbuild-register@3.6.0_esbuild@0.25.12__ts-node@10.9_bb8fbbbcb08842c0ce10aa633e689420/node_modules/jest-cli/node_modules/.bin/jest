#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ephrem/ephis_stuff/ephis_stuff/DS/ticket-management-service/node_modules/.pnpm/jest-cli@30.2.0_@types+node@25.0.3_esbuild-register@3.6.0_esbuild@0.25.12__ts-node@10.9_bb8fbbbcb08842c0ce10aa633e689420/node_modules/jest-cli/bin/node_modules:/home/ephrem/ephis_stuff/ephis_stuff/DS/ticket-management-service/node_modules/.pnpm/jest-cli@30.2.0_@types+node@25.0.3_esbuild-register@3.6.0_esbuild@0.25.12__ts-node@10.9_bb8fbbbcb08842c0ce10aa633e689420/node_modules/jest-cli/node_modules:/home/ephrem/ephis_stuff/ephis_stuff/DS/ticket-management-service/node_modules/.pnpm/jest-cli@30.2.0_@types+node@25.0.3_esbuild-register@3.6.0_esbuild@0.25.12__ts-node@10.9_bb8fbbbcb08842c0ce10aa633e689420/node_modules:/home/ephrem/ephis_stuff/ephis_stuff/DS/ticket-management-service/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ephrem/ephis_stuff/ephis_stuff/DS/ticket-management-service/node_modules/.pnpm/jest-cli@30.2.0_@types+node@25.0.3_esbuild-register@3.6.0_esbuild@0.25.12__ts-node@10.9_bb8fbbbcb08842c0ce10aa633e689420/node_modules/jest-cli/bin/node_modules:/home/ephrem/ephis_stuff/ephis_stuff/DS/ticket-management-service/node_modules/.pnpm/jest-cli@30.2.0_@types+node@25.0.3_esbuild-register@3.6.0_esbuild@0.25.12__ts-node@10.9_bb8fbbbcb08842c0ce10aa633e689420/node_modules/jest-cli/node_modules:/home/ephrem/ephis_stuff/ephis_stuff/DS/ticket-management-service/node_modules/.pnpm/jest-cli@30.2.0_@types+node@25.0.3_esbuild-register@3.6.0_esbuild@0.25.12__ts-node@10.9_bb8fbbbcb08842c0ce10aa633e689420/node_modules:/home/ephrem/ephis_stuff/ephis_stuff/DS/ticket-management-service/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jest.js" "$@"
else
  exec node  "$basedir/../../bin/jest.js" "$@"
fi
